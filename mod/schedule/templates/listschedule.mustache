<button type="button" class="btn btn-primary" onclick="location.href='{{backUrl}}'">Back To Course</button>
<br><br>
<div class="content-container">
    <b>{{scheduleName}}</b>
    <br><br>
    <table class="generaltable">
        <thead>
            <tr>
                <th>Team</th>
                <th>Selected Schedule</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            {{#groups}}
            <tr>
                <td>{{name}}</td>
                <td>
                    {{#chosendata}}{{chosendata}}{{/chosendata}}
                    {{^chosendata}} -not chosen yet- {{/chosendata}}
                </td>
                <td>
                    <button class="mod_schedule_view_detail_button btn btn-primary" value="{{scheduleid}}">check-in status</button>
                </td>
            </tr>
            {{/groups}}
        </tbody>
    </table>
</div>